<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/imgbtn"
        android:layout_width="118dp"
        android:layout_height="65dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="11dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView37"
        tools:srcCompat="@color/material_dynamic_neutral60" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="24dp"
        android:text="Tanggal / Jam"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgProduk2" />

    <TextView
        android:id="@+id/txToko2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:text="SeMUA Evelyn"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imgProduk2"
        app:layout_constraintTop_toBottomOf="@+id/txHarga2" />

    <TextView
        android:id="@+id/txProduk2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="128dp"
        android:text="Siger Sunda"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/imgProduk2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txHarga2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:text="Rp. 5.000.000,00"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imgProduk2"
        app:layout_constraintTop_toBottomOf="@+id/txProduk2" />

    <ImageView
        android:id="@+id/imgProduk2"
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnKembali2"
        app:srcCompat="@color/design_default_color_primary"
        tools:srcCompat="@color/design_default_color_primary" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toStartOf="parent"
